  "keywords": [
    "Jade",
    "Stylus",
    "Carousel",
    "Slider"
  ],
  "license": "MIT",
  "main": "./src/carousel.jade",
  "name": "jade-carousel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darryl-snow/carousel-css.git"
  },
  "scripts": {
    "start": "jade ./src/example.jade --out ./dist --pretty && stylus ./src/carousel.styl --out ./dist && autoprefixer-cli -o ./dist/carousel.css ./dist/carousel.css"
